{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SUSE SupportConfig",
	"patterns": [
		{
			"include": "#entity"
		},
		{
			"include": "#commentblock"
		},
		{
			"include": "#comment"
		}
	],
	"repository": {
		"entity": {
			"patterns": [{
				"name": "entity.other",
				"begin": "#=",
				"end": "=#",
				"patterns": [
					{
						"name": "emphasis",
						"match": "\\[ [\\w\\s]* \\]"
					}
				]
			}]
		},
		"commentblock": {
			"patterns": [{
				"name": "comment.block",
				"match": "^=*$"
			}]
		},
		"comment": {
			"patterns": [{
				"name": "keyword.control.less",
				"match": "^# .*$"
			}]
		}
	},
	"scopeName": "source.susesupportconfig"
}