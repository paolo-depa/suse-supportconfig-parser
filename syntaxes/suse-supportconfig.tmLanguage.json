{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SUSE SupportConfig",
	"patterns": [
		{
			"include": "#entity"
		},
		{
			"include": "#commentblock"
		},
		{
			"include": "#command"
		}
	],
	"repository": {
		"entity": {
			"patterns": [{
				"name": "comment.line",
				"begin": "#=",
				"end": "=#",
				"patterns": [
					{
						"name": "emphasis",
						"match": "\\[[\\w\\d\\s]*\\]"
					}
				]
			}]
		},
		"commentblock": {
			"patterns": [{
				"name": "comment.line",
				"match": "^=*$"
			}]
		},
		"command": {
			"patterns": [{
				"name": "keyword.control.less",
				"match": "^# (?!Verification Status).*$"
			}]
		}
	},
	"scopeName": "source.susesupportconfig"
}